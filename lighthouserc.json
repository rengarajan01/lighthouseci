{
  "ci": {
    "collect": {
      "url": [
        "http://localhost:3000"
      ],
      "startServerCommand": "npm start",
      "startServerReadyPattern": "Local app running at",
      "startServerReadyTimeout": 30000
    },
    "assert": {
      "preset": "lighthouse:recommended"
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
